<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>12</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>

		<Title>Global.Utilities</Title>
		<Version>1.1</Version>
		<PackageProjectUrl>https://github.com/LIN-Experimental/Global.Utilities</PackageProjectUrl>
		<Authors>Alexander Giraldo</Authors>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/LIN-Experimental/Global.Utilities</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>utilidades,web,generador,ip,validaciones,currency</PackageTags>
		<PackageIcon>icon.png</PackageIcon>

	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<None Update="icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>